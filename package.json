{
  "name": "codigo-postal-cli",
  "version": "2.0.0",
  "description": "Get postal code from Correos de Chile CLI",
  "main": "src",
  "scripts": {
    "prepublish": "eslint src"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "codigo-postal": "src/index.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lgaticaq/codigo-postal-cli.git"
  },
  "keywords": [
    "codigo postal",
    "postal code",
    "zip",
    "chile",
    "cli"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/codigo-postal-cli/issues"
  },
  "homepage": "https://github.com/lgaticaq/codigo-postal-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "codigo-postal": "^2.0.0",
    "commander": "^2.9.0",
    "copy-paste": "^1.3.0",
    "update-notifier": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^3.2.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        2,
        2
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-console": [
        0
      ]
    }
  }
}
